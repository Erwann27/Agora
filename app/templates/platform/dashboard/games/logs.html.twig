{% extends 'platform/dashboard/layout.html.twig' %}

{% block title %}Logs de la partie{% endblock %}

{% block body %}
    <h1 style="color: #333; font-size: 24px; margin-bottom: 20px;">Logs de la partie</h1>
    <ul style="list-style-type: none; padding-left: 0;">
        {% for log in logs %}
            <li style="margin-bottom: 10px; background-color: #f9f9f9; padding: 10px; border-radius: 5px;">{{ log.message }}</li>
        {% endfor %}
    </ul>
{% endblock %}